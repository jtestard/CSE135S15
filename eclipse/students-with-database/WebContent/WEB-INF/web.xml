<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE web-app
  PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
  "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>

	<!--  General Web Application Description -->
	<display-name>Data Entry Web App</display-name>

	<description>
		This application demonstrates how SQL queries are executed
		within JSP pages
	</description>

	<servlet>
		<servlet-name>attempt1</servlet-name>
		<jsp-file>/attempt1/students.jsp</jsp-file>
	</servlet>

	<servlet-mapping>
		<servlet-name>attempt1</servlet-name>
		<url-pattern>/attempt1</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>attempt2</servlet-name>
		<jsp-file>/attempt2/students.jsp</jsp-file>
	</servlet>

	<servlet-mapping>
		<servlet-name>attempt2</servlet-name>
		<url-pattern>/attempt2</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>attempt3</servlet-name>
		<jsp-file>/attempt3/students.jsp</jsp-file>
	</servlet>

	<servlet-mapping>
		<servlet-name>attempt3</servlet-name>
		<url-pattern>/attempt3</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>
			attempt1
		</welcome-file>
	</welcome-file-list>	
	
</web-app>
